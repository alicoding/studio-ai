{
  "name": "semantic-search",
  "version": "1.0.0",
  "description": "Standalone semantic search for code using ElectronHub embeddings",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/cli.ts",
    "test": "tsx src/test.ts",
    "index": "tsx src/cli.ts index",
    "search": "tsx src/cli.ts search",
    "prepublishOnly": "npm run build"
  },
  "bin": {
    "semantic-search": "./dist/cli.js"
  },
  "keywords": [
    "semantic-search",
    "code-search",
    "embeddings",
    "vector-search"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "commander": "^12.0.0",
    "glob": "^10.3.10",
    "dotenv": "^16.4.5"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}