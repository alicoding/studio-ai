{
  "mcpServers": {
    "studio-ai": {
      "command": "node",
      "args": ["./web/server/mcp/studio-ai/dist/index.js"],
      "env": {
        "STUDIO_AI_API": "http://localhost:3456/api"
      }
    },
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest", "--vision"]
    },
    "sequential-thinking": {
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-sequential-thinking"]
    },
    "langgraph/guides Docs": {
      "command": "npx",
      "args": ["mcp-remote", "https://langchain-ai.gitmcp.io/langgraph/guides/"]
    },
    "Context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp@latest"]
    }
  }
}
